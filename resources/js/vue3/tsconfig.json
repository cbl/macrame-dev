{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "lib": ["dom", "es6", "es2020"],
        "noImplicitAny": false,
        "sourceMap": false
    },
    "include": [
        "**/*.ts", 
        "**/*.d.ts", 
        "**/*.tsx", 
        "**/*.vue", 
        "**/*.js"
    ],
    "exclude": [
        "./node_modules"
    ]
}
